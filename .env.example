# AuditEng V2 Environment Configuration
# Copy this file to .env and update values

# Database (SQLite path, relative to project root)
DATABASE_URL=sqlite+aiosqlite:///./data/auditeng.db

# Server
HOST=0.0.0.0
PORT=8000
DEBUG=true

# Authentication (generate secure keys for production)
SECRET_KEY=your-secret-key-change-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=30

# External Services (configure in later phases)
# LANDINGAI_API_KEY=your-landingai-key
# ANTHROPIC_API_KEY=your-anthropic-key
